{
  "expo": {
    "name": "masa",
    "slug": "masa",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "scheme": "masa",
    "ios": {
      "appleTeamId": "37U34B89DU",
      "supportsTablet": true,
      "entitlements": {
        "com.apple.security.application-groups": "group.com.anonymous.masa"
      },
      "bundleIdentifier": "com.anonymous.masa"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.masa"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-asset",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#ffffff",
          "image": "./assets/splash-icon.png"
        }
      ],
      "react-native-edge-to-edge",
      "@bacons/apple-targets"
    ]
  }
}
