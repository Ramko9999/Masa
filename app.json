{
  "expo": {
    "name": "Masa",
    "slug": "masa",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "scheme": "masa",
    "ios": {
      "appleTeamId": "37U34B89DU",
      "supportsTablet": false,
      "entitlements": {
        "com.apple.security.application-groups": [
          "group.com.anonymous.masa"
        ]
      },
      "infoPlist": {
        "NSLocationAlwaysAndWhenInUseUsageDescription": "The app uses your location to calculate accurate Panchangam details and festival timings specific to your area.",
        "NSLocationAlwaysUsageDescription": "The app uses your location to calculate accurate Panchangam details and festival timings specific to your area.",
        "NSLocationWhenInUseUsageDescription": "The app uses your location to calculate accurate Panchangam details and festival timings specific to your area.",
        "NSLocationUsageDescription": "The app uses your location to calculate accurate Panchangam details and festival timings specific to your area."
      },
      "bundleIdentifier": "com.anonymous.masa",
      "icon": {
        "light": "./assets/logo/ios/light.png",
        "dark": "./assets/logo/ios/dark.png",
        "tinted": "./assets/logo/ios/tinted.png"
      }
    },
    "android": {
      "versionCode": 3,
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo/android/adaptive.png",
        "monochromeImage": "./assets/logo/android/adaptive.png",
        "backgroundColor": "#000000"
      },
      "package": "com.anonymous.masa"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-asset",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#ffffff",
          "image": "./assets/splash-icon.png"
        }
      ],
      [
        "react-native-edge-to-edge",
        {
          "android": {
            "enforceNavigationBarContrast": false
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "d47ed189-36f2-4c5b-abff-b7401ff7339c"
      }
    }
  }
}