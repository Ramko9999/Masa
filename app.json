{
  "expo": {
    "name": "Masa",
    "slug": "masa",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "scheme": "masa",
    "ios": {
      "appleTeamId": "37U34B89DU",
      "supportsTablet": true,
      "entitlements": {
        "com.apple.security.application-groups": "group.com.anonymous.masa"
      },
      "infoPlist": {
        "NSLocationUsageDescription": "We need your location to calculate the correct calendar elements and festival timings for your region.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "We need your location to calculate the correct calendar elements and festival timings for your region."
      },
      "bundleIdentifier": "com.anonymous.masa",
      "icon": {
        "light": "./assets/logo/ios/light.png",
        "dark": "./assets/logo/ios/dark.png",
        "tinted": "./assets/logo/ios/tinted.png"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo/android/adaptive.png",
        "monochromeImage": "./assets/logo/android/adaptive.png",
        "backgroundColor": "#000000"
      },
      "package": "com.anonymous.masa"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-asset",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "react-native-edge-to-edge",
        {
          "android": {
            "enforceNavigationBarContrast": false
          }
        }
      ]
    ]
  }
}